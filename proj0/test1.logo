@SIZE 1920 1080
@BACKGROUND 255 255 255
@POSITION 959 10

FUNC addLife(life_time,expected, count)
    LOOP count
    	ADD expected 1
    	LOOP count
			ADD life_time expected
		END LOOP
    END LOOP
END FUNC

FUNC editTime( )
	ADD TIME -1
END FUNC

DEF HAHA 1
DEF TIME 18
CALL addLife(TIME, HAHA,3)
CALL editTime(  )